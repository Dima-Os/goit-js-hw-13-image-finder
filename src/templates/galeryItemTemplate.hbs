{{#each this}}
<li class="photo-card col-3">
    <img src={{this.webformatURL}} width="210" height="220" class="image" data-source={{this.largeImageURL}} />

    <div class=" stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{this.likes}} </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{this.views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{this.comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{this.downloads}}
        </p>
    </div>
</li>
{{/each}}